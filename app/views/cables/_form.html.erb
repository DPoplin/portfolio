<%= form_with(model: cable) do |form| %>
    <div class="form-group">
    <%= form.label :build_date %>
    <%= form.datetime_field :build_date, :class => 'form-control' %>
    </div>
    <div class="form-group">
    <%= form.label :paracord %>
    <%= form.text_field :paracord, :class => 'form-control' %>
    </div>
    <div class="form-group">
    <%= form.label :techflex %>
    <%= form.text_field :techflex, :class => 'form-control' %>
    </div>
    <div class="form-group">
     <%= form.label :heatshrink %>
    <%= form.text_field :heatshrink, :class => 'form-control' %>
    </div>
    <div class="form-group">
      <%= form.label :connector %>
      <%= form.text_field :connector, :class => 'form-control' %>
    </div>
    <div class="form-group">
      <%= form.label :image %>
      <%= form.file_field :image, :class => 'form-control'%>
    </div>


  <% if cable.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(cable.errors.count, "error") %> prohibited this cable from being saved:</h2>

      <ul>
        <% cable.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
